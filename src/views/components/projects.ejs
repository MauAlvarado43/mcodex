<div class="projects-container" id="projects">

    <br><br><br><br>
    <div class="row projects-title">
        <div class="col">
            <h2><%= assets.nav.projects %></h2>
        </div>
    </div>
    <br><br>

    <div class="row">

        <div class="col">
            <div class="card" data-aos="zoom-in-up">
                <img class="card-img-top" src="./images/projects/cyclon.png" alt="Cyclon">
                <div class="card-body">

                    <h5 class="card-title">Cyclon</h5>

                    <p class="card-text">
                        <%= assets.index.projects.cyclon %>
                    </p>

                    <div class="badges-container">
                        <span class="badge badge-backend">Node.js</span>
                        <span class="badge badge-backend">Socket.io</span>
                        <span class="badge badge-backend">Django</span>
                        <span class="badge badge-mobile">Flutter</span>
                        <span class="badge badge-database">GraphQL</span>
                        <span class="badge badge-database">MongoDB</span>
                        <span class="badge badge-skills">Vue.js</span>
                    </div>

                    <br>

                    <div class="text-center">
                        <a href="https://cyclon-beta.herokuapp.com/home" target="blank" class="btn btn-primary"><%= assets.btns.visit %></a>
                    </div>

                </div>
            </div>
        </div>

        <div class="col col-card">
            <div class="card" data-aos="zoom-in-up">
                <img class="card-img-top" src="./images/projects/asno.png" alt="Asno">
                <div class="card-body">

                    <h5 class="card-title">ASNO</h5>

                    <p class="card-text">
                        <%= assets.index.projects.asno %>
